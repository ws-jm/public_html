	text-transform: none;
	letter-spacing: 0.8px;
	text-shadow: 0px 0px 4px rgba(0, 0, 0, 0.3);
	font-weight: 600;
}

.main-heading p {
	color: #999999;
	font-size: 18px;
	font-weight: 400;
}

.mandatory {
	display: inline !important;
	padding: 0 !important;
	margin: 0 !important;
