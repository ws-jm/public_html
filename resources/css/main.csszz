/* Sticky footer styles
-------------------------------------------------- */
body {
	background: #ececec;
}

html {
	position: relative;
	min-height: 100%;
	overflow-x: hidden;
	-webkit-text-size-adjust: none;
}

* {
	margin: 0;
	padding: 0;
	list-style: none;
	box-sizing: border-box;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
	-ms-box-sizing: border-box;
	-o-box-sizing: border-box;
}

.main-banner {
	background: url(../images/Blue-6666.jpg) no-repeat right bottom !important;
	background-size: auto 100% !important;
	position: relative;
	min-height: 482px;
}

.main-banner h1 {
	color: rgba(249, 246, 72, 0.98);

	font-size: 42px;
	text-transform: uppercase;
	text-shadow: 0px 0px 10px rgba(0, 0, 0, 0.3);
	margin: 0px 0px 10px;
	font-weight: 600;
	font-family: "Tahoma", Sans-serif;
}


.main-banner p {
	color: #fff;
	font-size: 15px;
	line-height: 24px;
}

.main-banner ul {
	padding: 30px 0 0 30px;
	margin: 0;
}

.main-banner ul li {
	display: block;
	color: #fff;
	position: relative;
	padding: 0 0 0 30px;
	margin: 0 0 12px 0;
	font-size: 15px;
}

.main-banner ul li:before {
	content: "\f058";
	position: absolute;
	left: 0;
	top: 0;
	font-family: 'FontAwesome';
	font-size: 20px;
}

/*.dekstop-computer{ position:absolute; bottom:0; right:0;     margin: -11px;}*/
.dekstop-computer {
	position: absolute;
	bottom: -41px;
	right: 13px;
	margin: 0;
	width: 272px;
	float: right;
}

.dekstop-computer img {
	max-width: 100%;
}

.relative {
	position: relative;
}

.main-banner-content {
	padding: 10px 0 30px;
	position: relative;
}

.padT60 {
	padding-top: 60px;
}

.padB45 {
	padding-bottom: 45px;
}

.dis-block {
